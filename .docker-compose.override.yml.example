version: '3.8'

services:
  server:
    environment:
      # Sobrescreva variáveis de ambiente aqui
      JWT_SECRET: your-custom-secret-key
      DB_PASSWORD: your-custom-password

  postgres:
    environment:
      POSTGRES_PASSWORD: your-custom-password
